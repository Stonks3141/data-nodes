{
  "targets": {
    "debug": {
      "outFile": "build/out.wasm",
      "textFile": "build/out.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "outFile": "build/out.wasm",
      "textFile": "build/out.wat",
      "sourceMap": false,
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  },
  "options": {
    "bindings": "raw",
		"enable": ["simd"]
  }
}